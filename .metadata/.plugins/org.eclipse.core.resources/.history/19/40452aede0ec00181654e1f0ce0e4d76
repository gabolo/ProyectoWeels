package proxy_final;

public class ProxySingleton {
	private static ProxySingleton unicaInstancia = null;
	  Usuario user;
	 public ProxySingleton(Usuario user)
	    {
	        this.user=user;
	    }

	public ProxySingleton reemplazarConstructora() {

	if (unicaInstancia == null)

	unicaInstancia = new ProxySingleton( user);

	return unicaInstancia;

	}
	
